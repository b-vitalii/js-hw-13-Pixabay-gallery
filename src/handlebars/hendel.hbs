{{#each this}}
<li>
    <div class="photo-card">
        <img class="photo" src="{{webformatURL}}" data-src="{{largeImageURL}}" alt="" height="250px" />

        <div class="stats">
            <p class="stats-item">
                <i class="material-icons">thumb_up</i>
                <span class="icons-span">{{likes}}</span>
            </p>
            <p class="stats-item">
                <i class="material-icons">visibility</i>
                <span class="icons-span">{{views}}</span>
            </p>
            <p class="stats-item">
                <i class="material-icons">comment</i>
                <span class="icons-span">{{comments}}</span>
            </p>
            <p class="stats-item">
                <i class="material-icons">cloud_download</i>
                <span class="icons-span">{{downloads}}</span>
            </p>
        </div>
    </div>
</li>
{{/each}}